# Ensure that user-defined headers are included with quotes, so that we can find them
# Don't use "std" namespace
- IncludeToQuotes:
    - __utility.hpp
    - concepts.hpp
    - coroutine.hpp
    - functional.hpp
    - stop_token.hpp
- NamespaceRename:
    from: "std::execution"
    to: "concore::detail::execution"
- NamespaceRename:
    from: "std"
    to: "concore::detail"
